@model Market_Store.Models.Homepage

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_LayoutAdminDashboard.cshtml";
}

<h1>Delete</h1>


<div class="container">


    <h3>Are you sure you want to delete this?</h3>
  
    <div class="row">

           
           
               
        <div class="card mb-3" >
            <img class="card-img-top" src="/Image/@Model.IMAGES_SLIDER" alt="Card image cap" >
            <div class="card-body">
                <h5 class="card-title">This is Big Title: @Model.TILTE_BIG</h5>
                <p class="card-text"> This is Small Tilte: @Model.TILTE_SMALL</p>
            </div>
          
        </div>
              
            

        
    </div>

    <div class="row">
         
          <form asp-action="Delete">
        <input type="hidden" asp-for="HomeId" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a class="btn btn-primary" asp-action="Index">Back to List</a>
    </form>
        

      
    </div>



</div>